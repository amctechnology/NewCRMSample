<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Salesforce</title>

</head>
<body>    
    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>Salesforce</p>
        </footer>
    </div>

    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js"></script>
    <script src="https://code.jquery.com/jquery-1.11.1.min.js"></script>
    <script src="https://code.jquery.com/ui/1.11.1/jquery-ui.min.js"></script>
    <link rel="stylesheet" href="https://code.jquery.com/ui/1.11.1/themes/smoothness/jquery-ui.css" /> 
    @{ string frameworkUrl = Environment.GetEnvironmentVariable("FRAMEWORK_URL");}
    @{var apiLocation = frameworkUrl + "/Scripts/bundles/AMCAppPluginAPI.js";}
    <script src="@apiLocation" type="text/javascript"></script>    
    <script src="~/Scripts/AMCScripts/SalesforcePlugin.js" type="text/javascript"></script>
    @RenderSection("scripts", required: false)
</body>
</html>
